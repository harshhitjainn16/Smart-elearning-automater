{
  "version": 2,
  "name": "smart-elearning-automator-frontend",
  "buildCommand": "cd smart-elearning-automator && npm install && npm run build",
  "outputDirectory": "smart-elearning-automator/build",
  "installCommand": "cd smart-elearning-automator && npm install",
  "framework": "create-react-app",
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000"
      }
    }
  ],
  "env": {
    "GENERATE_SOURCEMAP": "false"
  }
}